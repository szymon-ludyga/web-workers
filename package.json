{
  "name": "mdm",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "GIT_COMMIT_SHA=$(git rev-parse --short HEAD) nx run-many --target=build --prod --all --parallel",
    "fix-pg-native-dep": "node apps/server/test/ui/fixPgNativeDependency.js",
    "lint": "NODE_OPTIONS=--max_old_space_size=3072 nx affected --target=lint --base=origin/${CI_DEFAULT_BRANCH:-develop} --parallel=2 && npm run stylelint",
    "lint:all": "NODE_OPTIONS=--max_old_space_size=3072 nx run-many --target=lint --parallel=2 --all && npm run stylelint",
    "generate-prisma": "npm run generate-prisma:client && npm run generate-prisma:shared",
    "generate-prisma:client": "prisma generate --schema assets/schema.client.prisma",
    "generate-prisma:shared": "prisma generate --schema assets/schema.shared.prisma",
    "postinstall": "link-module-alias && npm run generate-prisma",
    "prepare": "is-ci || husky install",
    "pretest:ui:ci": "npm run fix-pg-native-dep && wait-on --timeout=300000 http://localhost:5000/api/health/ready",
    "pretest:ui:cypress": "npm run fix-pg-native-dep",
    "pretest:ui:headless": "npm run fix-pg-native-dep",
    "start:dev": "concurrently -n mock-data-service,data-import-service,server,migrations-service,client,users-service,clients-service -c green,yellow,blue,magenta,cyan,white,gray,red \"nx serve mock-data-service\" \"nx serve data-import-service | bunyan\" \"npm run start:server\" \"npm run start:migrations-service\" \"nx serve client\" \"npm run start:users-service\" \"npm run start:clients-service\"",
    "start:dev:lite": "concurrently -n server,migrations-service,client -c green,yellow,blue \"npm run start:server\" \"npm run start:migrations-service\" \"nx serve client\"",
    "start:dev:all": "concurrently -n mock-data-service,data-import-service,server,migrations-service,client,objects-service,jim-jobs-service,clients-service,users-service -c green,yellow,blue,magenta,cyan,white,gray,red \"nx serve mock-data-service\" \"nx serve data-import-service | bunyan\" \"npm run start:server\" \"npm run start:migrations-service\" \"nx serve client\" \"nx serve objects-service\" \"npm run start:jim-jobs-service\" \"npm run start:clients-service\" \"npm run start:users-service\"",
    "start:users-service": "nx serve users-service",
    "start:data-import-service": "nx serve data-import-service | bunyan",
    "start:jim-jobs-service": "env-cmd -f libs/jim-jobs/.env nx serve jim-jobs-service | bunyan",
    "start:migrations-service": "nx serve migrations-service | bunyan",
    "start:clients-service": "nx serve clients-service | bunyan",
    "start:server": "GIT_COMMIT_SHA=$(git rev-parse --short HEAD) nx serve server | bunyan",
    "stylelint": "stylelint --config apps/client/.stylelintrc 'apps/client/src/**/*.scss'",
    "stylelint:fix": "npm run stylelint -- --fix",
    "test": "nx affected --target=test --base=origin/${CI_DEFAULT_BRANCH:-develop} --parallel=3 --exclude=clients-service,migrations-service",
    "test:all": "nx run-many --target=test --all --parallel --exclude=clients-service,migrations-service",
    "test:api": "env-cmd -f apps/server/.env.test nx test-api server --args='test/api/**/*.{test,spec}.{js,ts}'",
    "test:api:environment": "concurrently -n migrations-service,mock-data-service,data-import-service,users-service,clients-service,server -c green,yellow,blue,magenta,cyan,white,gray,red \"env-cmd -f apps/migrations-service/.env.test npm run start:migrations-service\" \"nx serve mock-data-service\" \"env-cmd -f apps/data-import-service/.env.test npm run start:data-import-service\" \"env-cmd -f apps/users-service/.env.test npm run start:users-service\" \"env-cmd -f apps/clients-service/.env.test npm run start:clients-service\" \"env-cmd -f apps/server/.env.test npm run start:server\"",
    "test:api:ci": "wait-on --timeout=180000 http://localhost:5000/api/health/ready && npm run test:api",
    "test:api:full": "start-server-and-test 'npm run test:api:environment' http://localhost:5000/api/health/ready 'npm run test:api'",
    "test:api:single": "env-cmd -f apps/server/.env.test nx test-api server --args='$FILE'",
    "test:jim-jobs-service": "env-cmd -f libs/jim-jobs/.env.test nx test-e2e jim-jobs",
    "test:jim-jobs-service:environment": "concurrently -n migrations-service,jim-jobs-service -c green,yellow \"env-cmd -f apps/migrations-service/.env.test npm run start:migrations-service\" \"env-cmd -f libs/jim-jobs/.env.test nx serve jim-jobs-service | bunyan\"",
    "test:jim-jobs-service:full": "start-server-and-test 'npm run test:jim-jobs-service:environment' http://localhost:2000/healthcheck 'npm run test:jim-jobs-service'",
    "test:objects-service": "env-cmd -f apps/objects-service/.env.test nx test-e2e objects-service",
    "test:objects-service:environment": "env-cmd -f apps/migrations-service/.env.test npm run start:migrations-service",
    "test:objects-service:full": "concurrently -k -n migrations-service,objects-service -c green,yellow \"npm run test:objects-service:environment\" \"npm run test:objects-service\"",
    "test:ui:ci": "npm run test:ui:headless -- --record --parallel --key fake --ci-build-id $CYPRESS_BUILD_ID",
    "test:ui:cypress": "env-cmd -f apps/server/.env.test cypress open --project apps/server/test/ui",
    "test:ui:environment": "env-cmd -f apps/server/.env.test npm run start:dev",
    "test:ui:headless": "env-cmd -f apps/server/.env.test cy2 run --project apps/server/test/ui",
    "test:ui:single": "npm run test:ui:headless -- --spec=$FILE",
    "validate": "nx affected --target=check-types --base=origin/${CI_DEFAULT_BRANCH:-develop} --parallel && nx validate-schema migrations-service -- --log",
    "validate:all": "nx run-many --target=check-types --all --parallel && nx validate-schema migrations-service -- --log"
  },
